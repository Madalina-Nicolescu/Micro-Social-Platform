@model BDProiect.Models.Group

<h2>Editare grup</h2>

@using (Html.BeginForm(actionName: "Edit", controllerName: "Groups"))
{
    @Html.HttpMethodOverride(HttpVerbs.Put)

    @Html.Label("GroupName", "Nume Grup")
    <br />
    @Html.Editor("GroupName")
    <br />
    <br />
    @Html.Label("GroupDescription", "Descriere Grup")
    <br />
    @Html.Editor("GroupDescription")
    <br />
    <br />
    <button class="btn btn-success" type="submit">Modifica grup</button>
}

